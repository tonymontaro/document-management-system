{
  "name": "doc-mage",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node lib/server/index.js",
    "start:dev": "NODE_ENV=development nodemon --watch lib/server lib/server/index.js",
    "dev": "npm-run-all --parallel build:watch start:dev",
    "build:watch": "babel --watch server -d lib/server",
    "build": "rm -rf lib/server && babel server -d lib/server && npm run build:client",
    "build:server": "rm -rf lib/ && babel server -d lib/server",
    "build:client": "webpack --config webpack.config.prod.js && cat client/index.html > lib/client/index.html && cp -r api_docs lib/client",
    "test": "npm run setup:test && NODE_ENV=test nyc mocha server/test/**/*.js client/test/testSetup.js client/test/**/* --timeout 20000",
    "setup:test": "npm run db:migrate:test && NODE_ENV=test sequelize db:seed:all",
    "test:server": "npm run setup:test && NODE_ENV=test nyc mocha server/test/**/*.js --timeout 20000",
    "test:client": "NODE_ENV=test nyc mocha client/test/testSetup.js client/test/**/*",
    "test:client:watch": "mocha client/test/testSetup.js client/test/**/*.js --watch",
    "db:migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:migrate:test": "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate",
    "postinstall": "npm run build && npm run db:migrate && npm run db:seed",
    "e2e-setup": "selenium-standalone install",
    "e2e-test": "nightwatch"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "client/test",
      "server/test"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-angene/document-management-system.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela-angene/document-management-system/issues"
  },
  "homepage": "https://github.com/andela-angene/document-management-system#readme",
  "dependencies": {
    "axios": "^0.16.1",
    "babel-cli": "^6.24.1",
    "babel-polyfill": "6.8.0",
    "babel-preset-es2015": "^6.24.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.1",
    "compression": "1.6.1",
