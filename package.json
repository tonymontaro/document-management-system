    "eslint": "^3.19.0",
    "express": "^4.15.2",
    "faker": "^4.1.0",
    "html-truncate": "^1.2.2",
    "jquery": "2.2.3",
    "jsonwebtoken": "^7.4.0",
    "morgan": "^1.8.1",
    "pg": "^6.1.5",
    "pg-hstore": "^2.3.2",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-redux": "4.4.5",
    "react-render-html": "^0.1.6",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.4",
    "react-tinymce": "^0.5.1",
    "redux": "3.5.2",
    "redux-thunk": "2.0.1",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^2.7.0",
    "validator": "^7.0.0",
